<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios" table="Usuarios">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="text" column="username" length="65535" nullable="false"/>
    <field name="password" type="text" column="password" length="65535" nullable="false"/>
    <field name="habilitado" type="boolean" column="habilitado" nullable="false"/>
    <field name="rol" type="text" column="rol" length="65535" nullable="false"/>
  </entity>
</doctrine-mapping>
